<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>wheels.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>cacheA</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
    <object-name>axeAH</object-name>
    <object-name>axeAB</object-name>

    <object-name>roueA</object-name>
    <object-name>roueG</object-name>
    <object-name>roueD</object-name>
    <object-name>axeA</object-name>
    <object-name>intcacheA</object-name>
    <object-name>intcacheG</object-name>
    <object-name>intcacheD</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>material</type>
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>cacheA</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
    <object-name>axeAH</object-name>
    <object-name>axeAB</object-name>

    <object-name>roueA</object-name>
    <object-name>roueG</object-name>
    <object-name>roueD</object-name>
    <object-name>axeA</object-name>
    <object-name>intcacheA</object-name>
    <object-name>intcacheG</object-name>
    <object-name>intcacheD</object-name>

    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
<!--
    <diffuse>
      <red-prop>diffuse/red</red-prop>
      <green-prop>diffuse/green</green-prop>
      <blue-prop>diffuse/blue</blue-prop>
    </diffuse>

    <ambient>
      <red-prop>ambient/red</red-prop>
      <green-prop>ambient/green</green-prop>
      <blue-prop>ambient/blue</blue-prop>
    </ambient>
-->
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>

    <emission>
      <red-prop>emission/red</red-prop>
      <green-prop>emission/green</green-prop>
      <blue-prop>emission/blue</blue-prop>
    </emission>
<!--
    <shininess>
      <shininess-prop>shininess</shininess-prop>
    </shininess>
-->
  </animation>

  <effect>
    <inherits-from>Aircraft/Generic/Effects/Fuselagereflect</inherits-from>
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>cacheA</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
  </effect>

  <!-- Effets sur le sol -->
  <!-- Roue avant -->
  <model>
    <path>Aircraft/Tecnam-P92/Models/Effects/ground/ground0.xml</path>
    <offsets>
      <x-m> -2.581 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.242 </z-m>
    </offsets>
  </model>

  <!-- Roue gauche -->
  <model>
    <path>Aircraft/Tecnam-P92/Models/Effects/ground/ground1.xml</path>
    <offsets>
      <x-m> -1.017 </x-m>
      <y-m> -0.953 </y-m>
      <z-m> -1.242 </z-m>
    </offsets>
  </model>

  <!-- Roue droite -->
  <model>
    <path>Aircraft/Tecnam-P92/Models/Effects/ground/ground2.xml</path>   
    <offsets>
      <x-m> -1.017 </x-m>
      <y-m>  0.953 </y-m>
      <z-m> -1.242 </z-m>
    </offsets>
  </model>

  <!-- Compression -->
  <animation>
    <name>EnsembleA</name>
    <object-name>cacheA</object-name>
    <object-name>intcacheA</object-name>
    <object-name>roueA</object-name>
    <object-name>axeAH</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EnsembleA</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 10 </dep></entry>
    </interpolation>
    <center>
      <x-m> -2.735 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.735 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <name>EnsembleG</name>
    <object-name>cacheG</object-name>
    <object-name>intcacheG</object-name>
    <object-name>axeG</object-name>
    <object-name>roueG</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EnsembleG</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   5 </dep></entry>
      <entry><ind> 0.7 </ind><dep>  -2 </dep></entry>
      <entry><ind> 1.0 </ind><dep> -15 </dep></entry>
    </interpolation>
    <center>
      <x-m> -1.021 </x-m>
      <y-m> -0.460 </y-m>
      <z-m> -0.542 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <name>EnsembleD</name>
    <object-name>cacheD</object-name>
    <object-name>intcacheD</object-name>
    <object-name>axeD</object-name>
    <object-name>roueD</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EnsembleD</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   5 </dep></entry>
      <entry><ind> 0.7 </ind><dep>  -2 </dep></entry>
      <entry><ind> 1.0 </ind><dep> -15 </dep></entry>
    </interpolation>
    <center>
      <x-m> -1.021 </x-m>
      <y-m>  0.460 </y-m>
      <z-m> -0.542 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <name>RoueAvant</name>
    <object-name>roueA</object-name>
    <object-name>cacheA</object-name>
    <object-name>intcacheA</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RoueAvant</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> -10 </factor>
    <axis>
      <x1-m> -2.652 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m> -0.884 </z1-m>
      <x2-m> -2.579 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m> -0.570 </z2-m>
    </axis>
  </animation>

  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueA</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.581 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.096 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -1.017 </x-m>
      <y-m> -0.953 </y-m>
      <z-m> -1.096 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <property>gear/gear[2]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -1.017 </x-m>
      <y-m>  0.953 </y-m>
      <z-m> -1.096 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
