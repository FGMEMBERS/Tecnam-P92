<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>oil-pressure.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.9 </red>
      <green> 0.5 </green>
      <blue>  0.2 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/oil-pressure-psi</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   -1 </dep></entry>
      <entry><ind>  1 </ind><dep>   7 </dep></entry>
      <entry><ind>  2 </ind><dep>  15 </dep></entry>
      <entry><ind>  3 </ind><dep>  23 </dep></entry>
      <entry><ind>  4 </ind><dep>  33 </dep></entry>
      <entry><ind>  5 </ind><dep>  45 </dep></entry>
      <entry><ind>  6 </ind><dep>  56 </dep></entry>
      <entry><ind>  7 </ind><dep>  66 </dep></entry>
      <entry><ind>  8 </ind><dep>  74 </dep></entry>
      <entry><ind>  9 </ind><dep>  81 </dep></entry>
      <entry><ind> 10 </ind><dep>  86 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.010 </z-m>  
    </center>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
 
</PropertyList>
