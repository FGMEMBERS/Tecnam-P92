<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>kn53.ac</path>
 
  <animation>
    <object-name>freq.101</object-name>
    <object-name>freq.102</object-name>
    <object-name>freq.103</object-name>
    <object-name>freq.104</object-name>
    <object-name>freq.105</object-name>
    <object-name>stby.101</object-name>
    <object-name>stby.102</object-name>
    <object-name>stby.103</object-name>
    <object-name>stby.104</object-name>
    <object-name>stby.105</object-name>
    <object-name>KN53.dec</object-name>
    <object-name>KN53.swap</object-name>
    <object-name>KN53</object-name>
    <object-name>KN53.display</object-name>
    <object-name>KN53.knob</object-name>
    <object-name>KN53.knob1</object-name>
    <object-name>KN53.vol</object-name>
    <object-name>KN53.labels</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>KN53.swap</object-name>
    <object-name>KN53.knob</object-name>
    <emission>
      <factor-prop>/controls/lighting/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>freq.101</object-name>
    <object-name>freq.102</object-name>
    <object-name>freq.103</object-name>
    <object-name>freq.104</object-name>
    <object-name>freq.105</object-name>
    <object-name>stby.101</object-name>
    <object-name>stby.102</object-name>
    <object-name>stby.103</object-name>
    <object-name>stby.104</object-name>
    <object-name>stby.105</object-name>
    <emission>
      <factor-prop>/controls/lighting/instrument-lights</factor-prop>
      <red>   2 </red>
      <green> 2 </green>
      <blue>  2 </blue>
    </emission>
  </animation>
  
  <animation>
    <name>nav1-leds</name>
    <object-name>freq.101</object-name>
    <object-name>freq.102</object-name>
    <object-name>freq.103</object-name>
    <object-name>freq.104</object-name>
    <object-name>freq.105</object-name>
    <object-name>stby.101</object-name>
    <object-name>stby.102</object-name>
    <object-name>stby.103</object-name>
    <object-name>stby.104</object-name>
    <object-name>stby.105</object-name>
    <object-name>KN53.dec</object-name>
    <object-name>KN53.swap</object-name>
  </animation>
 
  <animation>
    <type>select</type>
    <object-name>nav1-leds</object-name>
    <condition>
      <and>
        <equals>
          <property>instrumentation/nav/serviceable</property>
          <value>true</value>
        </equals>
        <greater-than>
          <property>systems/electrical/outputs/nav</property>
          <value>1</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <!--                    FREQUENCY                                       -->
  <animation>
    <type>textranslate</type>
    <object-name>freq.101</object-name>
    <property>instrumentation/nav/frequencies/selected-mhz</property>
    <factor>0.001</factor>
    <step>100</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>freq.102</object-name>
    <property>instrumentation/nav/frequencies/selected-mhz</property>
    <factor>0.01</factor>
    <step>10</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>freq.103</object-name>
    <property>instrumentation/nav/frequencies/selected-mhz</property>
    <factor>0.1</factor>
    <step>1</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>freq.104</object-name>
    <property>instrumentation/nav/frequencies/selected-mhz</property>
    <factor>1.0</factor>
    <step>0.1</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>freq.105</object-name>
    <property>instrumentation/nav/frequencies/selected-mhz</property>
    <factor>10.0</factor>
    <step>0.01</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <!--         STANDBY         -->
  <animation>
    <type>textranslate</type>
    <object-name>stby.101</object-name>
    <property>instrumentation/nav/frequencies/standby-mhz</property>
    <factor>0.001</factor>
    <step>100</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>stby.102</object-name>
    <property>instrumentation/nav/frequencies/standby-mhz</property>
    <factor>0.01</factor>
    <step>10</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>stby.103</object-name>
    <property>instrumentation/nav/frequencies/standby-mhz</property>
    <factor>0.1</factor>
    <step>1</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>stby.104</object-name>
    <property>instrumentation/nav/frequencies/standby-mhz</property>
    <factor>1.0</factor>
    <step>0.1</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>stby.105</object-name>
    <property>instrumentation/nav/frequencies/standby-mhz</property>
    <factor>10.0</factor>
    <step>0.01</step>
    <bias>0.005</bias>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>KN53.vol</object-name>
    <property>instrumentation/nav/volume</property>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>1.0</ind><dep>300</dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.038 </y-m>
      <z-m> -0.006 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!--    OSG Hotspots    -->
  <animation>
    <type>pick</type>
    <object-name>KN53.swap</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-swap</command>
        <property>instrumentation/nav/frequencies/selected-mhz</property>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
      </binding>
    </action>
  </animation>
    
  <animation>
    <type>pick</type>
    <object-name>KN53.vol</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/volume</property>
        <step>0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.vol</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/volume</property>
        <step>-0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.vol</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/volume</property>
        <step>0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.vol</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/volume</property>
        <step>-0.05</step>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob1</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>0.05</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob1</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>-0.05</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob1</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>0.05</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob1</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>-0.05</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>1.00</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.2</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>-1.00</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob</object-name>
    <action>
      <button>3</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav/frequencies/standby-mhz</property>
        <step>1.00</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>KN53.knob</object-name>
    <action>
      <button>4</button>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/nav//frequencies/standby-mhz</property>
        <step>-1.00</step>
        <min>108.00</min>
        <max>118.00</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

</PropertyList>
