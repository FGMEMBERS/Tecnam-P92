<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>lamp.ac</path>

  <animation>
    <object-name>lampoff</object-name>
    <object-name>lamponb</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>lampoff</object-name>
    <emission>
      <red>   0 </red>
      <green> 0 </green>
      <blue>  0 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>lampon</object-name>
    <emission>
      <red>   2 </red>
      <green> 2 </green>
      <blue>  2 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>lampon</object-name>
    <condition>
      <equals>
        <property>/controls/electric/engine/generator</property>
        <value> false </value>
      </equals>
    </condition>
  </animation>

</PropertyList>
