<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="p92-base.xml">
  <sim>

    <description>Tecnam P92 Echo Wheels (YASim)</description>
    <flight-model archive="y">yasim</flight-model>
    <aero archive="y">p92W-yasim</aero>
    
    <rating>
      <FDM type="int">3</FDM>
      <systems type="int">4</systems>
      <cockpit type="int">4</cockpit>
      <model type="int">4</model>
    </rating>
    
    <startup>
      <splash-texture>Aircraft/Tecnam-P92/Splash/p92W-splash.png</splash-texture>
    </startup>
    
    <sound>
      <path archive="y">Aircraft/Tecnam-P92/Sounds/p92W-sound.xml</path>
    </sound>
    
    <model>
      <path archive="y">Aircraft/Tecnam-P92/Models/p92W.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>
    
  </sim>

</PropertyList>
